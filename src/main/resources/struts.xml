<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="demo" extends="struts-default">
        <action name="loginForm" class="web.AccountAction">
            <result name="input">/WEB-INF/jsp/Account/login.jsp</result>
        </action>

        <action name="login" class="web.AccountAction" method="login">
            <result name="input">/WEB-INF/jsp/Account/login.jsp</result>
            <result name="success">/WEB-INF/jsp/Account/result.jsp</result>
        </action>
    </package>
</struts>